    "nibandhanaigal<br />\nyaedhum ennil illai<br />\nkaaranangal illai<br />\nkaetkaadhe sonnaalum<br />\nyaerkkaadhe. ] (2)",
    "Idhayane ennai enna seigiraai<br />\ninimaigal enil seidhu ponaai",
    "idhayane enna maayam seigiraai<br />\niravugal vellaiyaakki ponaai",
